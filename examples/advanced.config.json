{
  "project": {
    "repo": "owner/repo",
    "path": "/path/to/your/repository",
    "baseBranch": "develop"
  },
  "executor": {
    "maxParallel": 5,
    "timeoutMinutes": 45,
    "createPr": true,
    "prDraft": true
  },
  "planner": {
    "enabled": true,
    "model": "opus",
    "maxBudgetUsd": 3.0,
    "maxTurns": 10
  },
  "agent": {
    "model": "sonnet",
    "maxBudgetUsd": 8.0,
    "yolo": false,
    "maxTurns": 12
  },
  "maxTotalBudgetUsd": 100.0,
  "telegram": {
    "enabled": true,
    "token": "YOUR_TELEGRAM_BOT_TOKEN",
    "allowedUserIds": [
      123456789,
      987654321
    ],
    "health": {
      "enabled": true,
      "port": 3000,
      "bindAddress": "0.0.0.0"
    }
  },
  "dashboard": {
    "enabled": true,
    "port": 3030
  }
}
